
@{
    ViewBag.Title = "CustomerLogin";
}


<h2 align="center">Customer Login</h2>
<hr />
<div class="container">
    <div class="col-md-6">
        <img src="~/images/cl.png" class="img-responsive" />
    </div>
    <div class="col-md-6">
        <form method="post" action="LoginPass">
            <div class="form-group">
                <label>Enter User Name </label> <br />
                <input type="email" name="CustomerName" class="form-control" />
            </div>

            <div class="form-group">
                <label>Enter User Password </label> <br />
                <input type="password" name="CustomerPassword" class="form-control" />
            </div>

            <input type="submit" value="submit" class="btn btn-primary" />
        </form>

    </div>
</div>



